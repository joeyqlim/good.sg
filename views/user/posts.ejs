<div class="container text-center">
  <div class="row mt-3">
    <div class="col-md-10 mx-auto">
      <h6>Your posts</h6>
      <% posts.posts.slice().reverse().forEach((post)=>{ %>
        <div class="card">
          <div class="card-body">
            <p class="card-subtitle mb-2 text-muted small text-left"><%= post.postDate %></p>
            <p class="text-left"><span class="font-weight-bold"><%= posts.username %>: </span><span class="post-text"><%= post.text %></span></p>
          </div>
        </div>
      <% }) %>
    </div>
  </div>
  </div>
